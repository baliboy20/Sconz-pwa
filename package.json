{
  "name": "zz-test-mat-hints",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --stats-json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "scully:scan": "npx scully -- --scan-routes",
    "del": "del-cli --force ./dist",
    "delb4a": "del-cli --force ./test22/public",
    "cpy-static": "rm -rf ./test22/public && cp -a ./dist/static ./test22/public",
    "cpy-non-static": "rm -rf ./test22/public && cp -a ./dist/RosyLookLike1 ./test22/public",
    "rm": "rm -rf ./test22/public",
    "build-deploy-scullied": "rm -rf ./dist && ng build --configuration production --stats-json && npm run scully:scan && npm run copy-deploy-scullied-only",
    "copy-deploy-scullied-only": "npm run cpy-static && cd ./test22 && b4a deploy > ../ZDEPLOYED.TXT && cd .. && echo `date` > ZUPLOAD_COMPLETE.TXT",
    "nobuild-cpy-scullied-deploy": "npm run cpy-static && cd ./test22 && b4a deploy && cd .. && echo `date` > LAST_DEPLOYMENT_LOG.TXT",
    "build-nodeploy-scullied": "rm -rf ./dist && ng build --configuration production && npm run scully:scan && npm run cpy-static",
    "build-deploy-unscullied": "rm -rf ./dist && ng build --configuration production &&  npm run cpy-non-static && cd ./test22 && b4a deploy && cd ..",
    "complete": "echo done",
    "buildout": "npm run build-deploy-scullied"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.o",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.2",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@scullyio/init": "^1.0.1",
    "@scullyio/ng-lib": "^1.0.0",
    "@scullyio/scully": "^1.0.0",
    "crypto-js": "^4.1.1",
    "http-server": "^13.0.0",
    "parse": "^2.19.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular/cli": "~12.2.0",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5",
    "@types/parse": "^2.18.6"
  }
}
